{
  "name": "gpwebpay-node-sdk",
  "version": "1.2.0",
  "description": "A TypeScript SDK for interacting with the GP WebPay payment gateway.",
  "author": "Federico Diaz <contact@fdiaz.dev>",
  "license": "MIT",
  "keywords": [
    "gpwebpay",
    "payments",
    "sdk",
    "typescript",
    "nodejs",
    "payment-gateway",
    "czech",
    "gp webpay"
  ],
  "bugs": {
    "url": "https://github.com/fedediiaz/gpwebpay-node-sdk/issues"
  },
  "homepage": "https://github.com/fedediiaz/gpwebpay-node-sdk#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm -C packages/core build && mkdir -p dist && cp -r packages/core/dist/* dist/",
    "test": "pnpm -C packages/core test",
    "typecheck": "pnpm -C packages/core typecheck",
    "lint": "pnpm -C packages/core lint",
    "format": "pnpm -C packages/core format",
    "format:fix": "pnpm -C packages/core format:fix",
    "prepublishOnly": "pnpm build"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321",
  "devDependencies": {
    "lerna": "^8.2.3"
  }
}